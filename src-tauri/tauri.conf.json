{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tcad",
  "version": "0.4.0",
  "identifier": "com.tcad.desktop",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "tcad",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEMzM0VDMDBCMkQ4NDE4QUUKUldTdUdJUXRDOEErdzZQYXVhU0thb0p3N2dtM0hmOFNqVVZ4RTQ0Wm1McHNhUTkxRWpPNGtlZWcK",
      "endpoints": [
        "https://github.com/eggfriedrice24/tcad/releases/latest/download/latest.json"
      ]
    }
  }
}
